    "test": "echo \"Error: no test specified\" && exit 1",
    "echo": "echo 'rebuild'",
    "build:bundle": "webpack --watch",
    "build": "npm run build:codec && npm run build:bundle",
    "serve": "http-server --experimental-wasi-unstable-preview1",
    "start": "npm run build && npm run serve",
    "dev-server": "webpack --watch --config webpack.dev.js",
    "---------- Normal Example commmaneline templete generator-------------------------------------------": "",
    "generate": "node ./tools/generate.js",
    "batchGenerate": "node ./tools/batchGenerate.js",
    "---------- Command Line Example -------------------------------------": "",
