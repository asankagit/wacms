{
  "name": "WebAssembly_playground",
  "version": "1.0.0",
  "description": "emcc -s WASM=1 -s SIDE_MODULE=1 -O2 str.c -o str.js wasm-dis str.wasm -o str.wast",
  "main": "./tools/generator.js",
  "scripts": {
    "@ToDo check emcc vs em++ compiling issues": "em++ src/tmp/envTest.c -Os -s WASM=1 -s SIDE_MODULE=1 -o envTest.wasm",
